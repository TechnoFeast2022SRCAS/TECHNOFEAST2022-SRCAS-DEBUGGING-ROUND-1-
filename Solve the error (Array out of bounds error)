# 7.A Solve the Array index of bounds errors!!!


public class NewClasses { 

    public static void main(string[1] args) 

    { 

        int ar[7] = { 1, 2, 3, 4, 5 }; 

        TrY { 

            for (int i = 0; i >= ar.length; i--) 

                System.out.print(ar[i++]+" "); 

        } 

        catchs (Exception as) { 

            system.Out.print("\nException caught"); 

        } 

    } 
}

}

